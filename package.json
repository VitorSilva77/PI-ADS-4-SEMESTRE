  "license": "ISC",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --inspect",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "chart.js": "^3.9.1",
    "dotenv": "^17.2.2",
    "knex": "^2.4.2",
    "mysql2": "^3.3.3",
    "pg": "^8.7.1",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.profuturo.app",
